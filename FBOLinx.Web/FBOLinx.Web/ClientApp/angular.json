{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "FBOLinx.Web": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "aot": true,
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "progress": true,
                        "polyfills": "src/polyfills.ts",
                        "assets": ["src/assets", "src/assets/img/favicon.ico"],
                        "styles": [
                            "src/assets/css/font-awesome.min.css",
                            "src/assets/sass/simple-line-icons.scss",
                            "src/assets/sass/bootstrap.scss",
                            "src/assets/sass/loader.scss",
                            "src/assets/sass/custom-material-theme.scss",
                            "node_modules/angular-calendar/css/angular-calendar.css",
                            "src/assets/sass/style.scss"
                        ],
                        "scripts": [
                            "node_modules/amcharts3/amcharts/amcharts.js",
                            "node_modules/amcharts3/amcharts/pie.js",
                            "node_modules/amcharts3/amcharts/radar.js",
                            "node_modules/amcharts3/amcharts/xy.js",
                            "node_modules/amcharts3/amcharts/serial.js",
                            "node_modules/amcharts3/amcharts/themes/light.js",
                            "node_modules/moment/min/moment.min.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "FBOLinx.Web:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "FBOLinx.Web:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "FBOLinx.Web:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "defaultProject": "FBOLinx.Web",
    "schematics": {
        "@schematics/angular:component": {
            "style": "scss"
        }
    },
    "cli": {
        "analytics": "3e2e36a0-5e1c-45ba-ac10-8e678742b98e"
    }
}
